    "def sum_of_n_number(n):\n",
    "    sum = 0\n",
    "    for i in range(1,n+1):\n",
    "        sum += i\n",
    "    return sum\n",
    "\n",
    "\n",
    "num  =  int(input(\"enter your number->\"))\n",
    "print(sum_of_n_number(num))"
