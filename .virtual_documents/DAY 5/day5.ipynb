import os
import pywhatkit

choice = input("""
1. Open Notepad
2. Open Chrome
3. Send WhatsApp
Enter choice: """)

if choice == "1":
    os.system("notepad.exe")
elif choice == "2":
    os.system("chrome.exe")
elif choice == "3":
    pywhatkit.sendwhatmsg("+1234567890", "Hello!", 15, 30)
else:
    print("Invalid choice")


